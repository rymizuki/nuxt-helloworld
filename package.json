{
  "name": "nuxt-helloworld",
  "version": "1.0.0",
  "description": "Example for Nuxt.js",
  "author": "mizuki_r <ry.mizuki@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
